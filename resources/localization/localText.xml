		<toolTip label="Pausar proyección de diapositivas" item="SlideshowVCRPauseTip"/>
		<toolTip label="Visualizar galería" item="SlideshowViewGalleryTip"/>
		<toolTip label="Visualizar proyección de diapositivas" item="SlideshowViewSlideshowTip"/>
		<toolTip label="Añadir comentario" item="SlideshowOpenCommentTip"/>
		<toolTip label="Marcar como favorita" item="SlideshowMarkFavoriteTip"/>
		<toolTip label="Página siguiente" item="ThumbnailNextPageTip"/>
		<toolTip label="Página anterior" item="ThumbnailPrevPageTip"/>
		<toolTip label="Última página" item="ThumbnailLastPageTip"/>
		<toolTip label="Primera página" item="ThumbnailFirstPageTip"/>
		<toolTip label="Reproducir vídeo" item="SlideshowVideoPlayTip"/>
		<toolTip label="Pausar vídeo" item="SlideshowVideoPauseTip"/>
