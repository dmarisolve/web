<?xml version="1.0"?>
<localization>
	<progressBars>
		<loadingImgTxt label="Cargando imagen..." />
		<loadingSizeTxt label="Cargando nuevo tamaño de imagen..." />
	</progressBars>
	<galleryMenus>
		<menuitem label="Vista" event="MenuView">
			<menuitem label="Galería" type="radio" event="MenuViewGallery"/>
			<menuitem label="Proyección de diapositivas" type="radio" event="MenuViewSlideshow"/>
			<menuitem label="Separator" event="Separator"/>
			<menuitem label="Acerca de estas fotografías" type="radio" event="MenuViewAbout"/>		
		</menuitem>
	</galleryMenus>
